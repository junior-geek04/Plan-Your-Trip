<?php
include_once('config.php');
if(isset($_POST['submit']))
{
$trip=$_POST['tri'];
$names=$_POST['name'];
$emails=$_POST['email'];
$contacts=$_POST['contact'];
$size=$_POST['size'];
$placevisit=$_POST['place'];
$budgets=$_POST['budg'];

$query=mysqli_query($con,"insert into tripz(Trip,Name,Email,Contact,groups,Place,Budget) value('$trip','$names','$emails','$contacts','$size','$placevisit','$budgets')");
echo "<script>alert('Your information succesfully submitted');</script>";
 /*EMAIL CODE*/  
 
    $formcontent="From: $names \n Trip: $trip \n Email: $emails \n Contact: $contacts \n Size: $size \n Place: $placevisit \n Budget: $budgets ";
            $recipient = "connect.amt@gmail.com";
            $subjects="Trips-form";
		// 	$sql="delete from tripz where Name='divyansh04';"; //DELETION
        //    $result=$con->query($sql);//DELETION

            mail($recipient, $subjects, $formcontent) or die("Error!");  
echo "<script>window.location.href ='Trips.php'</script>";

}


